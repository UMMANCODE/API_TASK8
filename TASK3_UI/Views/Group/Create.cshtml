@model GroupCreateRequest
@{
  ViewData["Title"] = "Create";
}
<div class="container-fluid">
  <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Group Create</h1>

  </div>
  <div class="row">
    <div class="col-md-8 mx-auto">
      <form class="user" asp-action="create" method="post" enctype="multipart/form-data">
        <div class="form-group">
          <label for="date">Name</label>
          <input asp-for="Name" class="form-control" id="date" placeholder=" ">
          <span class="text-danger" asp-validation-for="Name"></span>
        </div>

        <div class="form-group">
          <label for="descInput">Limit</label>
          <input asp-for="Limit" class="form-control" id="descInput" placeholder=" ">
          <span class="text-danger" asp-validation-for="Limit"></span>

        </div>

        <button class="btn btn-primary" type="submit">
          Create Group
        </button>
      </form>
    </div>
  </div>
</div>
@section scripts {
  @Html.Partial("_ValidationScriptsPartial");
}
